SET DATE ITALIAN
CLEAR
? 'RECIBOS'
USE REBUTS INDEX REBUTS,REBANC,RECLI,RE_DAT,LIQUIDA,RECO,LIQUI2,LIQUI3,REIVA,RESUM
GO TOP
WAIT
CONTA=1
@ 10,10 SAY CONTA
DO WHILE !EOF()
   IF ESTAT='L'.AND.DATAX<=CTOD('31-12-90')
	    DELETE
	    CONTA=CONTA+1
			@ 10,10 SAY CONTA
	 ENDIF
	 SKIP
ENDDO
WAIT
PACK
CLOSE ALL
USE PAGAMENT INDEX PAGA_DAT,PAGA_CLI,PAGA_NUM,REPAGA,PAIVA
?
?
? 'PAGAMENT'
WAIT
GO TOP
CONTA=1
@ 10,10 SAY CONTA
DO WHILE !EOF()
   IF ESTAT='L'.AND.DATAX<=CTOD('31-12-90')
	    DELETE
	    CONTA=CONTA+1
			@ 10,10 SAY CONTA
	 ENDIF
	 SKIP
ENDDO
WAIT
PACK
CLOSE ALL
QUIT